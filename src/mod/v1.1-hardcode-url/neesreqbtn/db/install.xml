<?xml version="1.0" encoding="UTF-8"?>
<XMLDB
    PATH="mod/neesreqbtn/db"
    VERSION="20241007"
    COMMENT="XMLDB file for Moodle mod/neesreqbtn"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd">
    <TABLES>
        <TABLE NAME="neesreqbtn" COMMENT="Stores instances of the NEES requisition button activity">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" UNSIGNED="true" NOTNULL="true" SEQUENCE="true"/>
                <FIELD NAME="course" TYPE="int" LENGTH="10" UNSIGNED="true" NOTNULL="true"/>
                <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true"/>
                <FIELD NAME="intro" TYPE="text" NOTNULL="false"/>
                <FIELD NAME="introformat" TYPE="int" LENGTH="4" UNSIGNED="true" NOTNULL="true"/>
                <FIELD NAME="buttontext" TYPE="char" LENGTH="255" NOTNULL="true"/>
                <FIELD NAME="timemodified" TYPE="int" LENGTH="10" UNSIGNED="true" NOTNULL="true"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
            </KEYS>
        </TABLE>
    </TABLES>
</XMLDB>
